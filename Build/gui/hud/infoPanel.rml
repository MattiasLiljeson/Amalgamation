<rml>
	<head>
		<link type="text/rcss" href="infoPanel.rcss"/>
		<title>Datagrid</title>
	</head>
	<body id="infoPanel">
		<div id="context">
			<datagrid source="playerstats.infopanel">
				<col fields="playerName" width="50%">Player:</col>
				<col fields="score" width="30%">Score:</col>
				<col fields="ping" width="20%">Ping:</col>
			</datagrid>
		</div>
	</body>
</rml>
